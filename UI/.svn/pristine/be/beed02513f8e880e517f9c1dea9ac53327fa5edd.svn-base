using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using UI.Model;

namespace UI.Communication.Events
{

   /// <summary>
    /// Event arguments that are used for an event after an Update is recieved
   /// </summary>
    public class TileEventArgs: System.EventArgs
    {
        public readonly Tile tile;

        public TileEventArgs(Tile tile)
        {
            this.tile = tile;
        }
    }
}
